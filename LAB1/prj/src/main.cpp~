//main.cpp
#include <iostream>
#include "program.hh"
#include "tabx2.hh"
#include "benchmark.hh"

using namespace std;

int main(){
  Tabx2 a;
  Benchmark b;
  b.rozpocznij_pomiar();
  char* nazwa = (char*)"dane.dat";
  a.wczytaj_dane(nazwa);
  a.wyswietl_dane();
  a.wykonaj_program();
  a.wyswietl_dane();
  nazwa = (char*)"dane_wy.dat";
  a.zapisz_dane(nazwa);
  b.zakoncz_pomiar();
  cout<<b.czas_pomiaru<<endl;

 
  return 0;
}
